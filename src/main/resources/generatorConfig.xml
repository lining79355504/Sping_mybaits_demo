<generatorConfiguration>
    <!-- 数据库驱动包 -->
    <classPathEntry location="/Users/mort/Sping_mybaits_demo/src/jdbcdriver/mysql-connector-java-5.1.39-bin.jar" />
    <context id="aisSnsTables" targetRuntime="MyBatis3">
